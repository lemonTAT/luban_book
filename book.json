{
  "gitbook": ">=3.0.0",
  "language": "zh-hans",
  "title": "鲁班应用搭建平台",
  "author": "178518 <178518@gmail.com>",
  "description": "鲁班应用搭建平台",
  "root": "./src",
  "links": {
    "sidebar": {
      "鲁班应用搭建平台": "https://github.com/178518/luban_book"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "-sharing",
    "-highlight",
    "splitter",
    "search-plus",
    "prism-ext",
    "github",
    "editlink",
    "advanced-emoji",
    "tbfed-pagefooter",
    "theme-comscore",
    "theme-api",
    "disqus",
    "copy-code-button",
    "donate",
    "alerts",
    "include-codeblock"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "github": {
      "url": "https://github.com/178518/luban_book"
    },
    "editlink": {
      "base": "https://github.com/178518/luban_book/tree/master/src",
      "label": "编辑本页"
    },
    "prism": {
      "theme": "prism-themes/themes/prism-duotone-dark.css",
      "plugins": ["line-numbers", "line-highlight"],
      "cssClasses": "line-numbers",
      "codeBlockExtSyntax": true,
      "lineHeight": 20
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright @2019",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "disqus": {
      "shortName": "luban"
    },
    "theme-api": {
      "theme": "dark"
    },
    "donate": {
      "wechat": "/images/pay/wechatpay.jpg",
      "alipay": "/images/pay/alipay.jpg",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "theme": "monokai"
    }
  }
}
